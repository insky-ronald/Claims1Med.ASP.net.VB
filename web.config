					<conditions logicalGrouping="MatchAny">
						<add input="{R:1}" pattern="^app$" />
						<add input="{R:1}" pattern="^sys$" />
					</conditions>
					<action type="Rewrite" url="/engine/content_loader.aspx?path={R:1}&amp;pid={R:2}&amp;keyid={R:3}" appendQueryString="true" />
