					</conditions>
					<action type="Rewrite" url="/{R:1}/main-{R:3}.ashx?pid={R:3}" appendQueryString="true" />
				</rule>

				<rule name="Sub Page Data Loader" stopProcessing="true">
					<match url="([_0-9a-z-]+)/([_0-9a-z-]+)/([_0-9a-z-]+)/([_0-9a-z-]+)" />
